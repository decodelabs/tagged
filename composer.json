{
    "name": "decodelabs/tagged",
    "description": "PHP markup generation without the fuss",
    "type": "library",
    "keywords": ["markup", "html", "generator"],
    "license": "MIT",
    "authors": [{
        "name": "Tom Wright",
        "email": "tom@inflatablecookie.com"
    }],
    "require": {
        "php": "^8.0",
        "ext-intl": "*",
        "symfony/polyfill-mbstring": "^1.7",

        "decodelabs/coercion": "^0.2",
        "decodelabs/collections": "^0.8",
        "decodelabs/cosmos": "^0.1.2",
        "decodelabs/elementary": "^0.2",
        "decodelabs/exceptional": "^0.4",
        "decodelabs/glitch-support": "^0.4",
        "decodelabs/veneer": "^0.10.10"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.6"
    },
    "suggest": {
        "nesbot/carbon": "Support for time interval formatting"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Tagged\\": "src/Tagged"
        },
        "files": [
            "src/global.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.14.x-dev"
        }
    },
    "repositories": {
    }
}
