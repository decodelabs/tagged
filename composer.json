{
    "name": "decodelabs/tagged",
    "description": "PHP markup generation without the fuss",
    "type": "library",
    "keywords": [ "markup", "html", "generator" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",
        "ext-intl": "*",
        "symfony/polyfill-mbstring": "^1.32",

        "decodelabs/archetype": "^0.3.10",
        "decodelabs/coercion": "^0.3.3",
        "decodelabs/collections": "^0.11.3",
        "decodelabs/cosmos": "^0.2.2",
        "decodelabs/elementary": "^0.6.1",
        "decodelabs/exceptional": "^0.6.1",
        "decodelabs/monarch": "^0.1.4",
        "decodelabs/veneer": "^0.12.8"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "suggest": {
        "nesbot/carbon": "Support for time interval formatting"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Tagged\\": "src/Tagged",
            "DecodeLabs\\PHPStan\\": "src/PHPStan"
        },
        "files": [
            "src/Tagged/Factory.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\Tagged\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.20.x-dev"
        },
        "effigy": {
            "exports": [
                "phpstan-extension.neon"
            ]
        },
        "phpstan": {
            "includes": [
                "phpstan-extension.neon"
            ]
        }
    },
    "repositories": {
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
